{
  "expo": {
    "name": "Boost",
    "slug": "Testosterone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/BoostLogo.png",
    "userInterfaceStyle": "dark",
    "scheme": "Boost",
    "splash": {
      "image": "./assets/BoostLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company.testosterone",
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default",
          "Default"
        ]
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7a74b131-1cfb-4e8a-a090-aa7d15ecfe2b"
      }
    }
  }
}
